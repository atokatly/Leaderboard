<%= form_for @score do |f| %>
  1 = Outlaw (Portal Mansion) , 2 = Tokyo (Rag Doll), 3 = Aztec (Lost Woods), 4 = Voyager (Bride's Game)
  <%= f.label :Challenge_Number %>
  <%= f.number_field :challengeboard_id %>
  <%= f.label :team_name %>
  <%= f.text_field :name %>
  <%= f.label :completion_time %>
  <%= f.text_field :time %>
  <div class="field">
    add a team image....
    <%= f.file_field :photo %>
  </div>
  <%=f.submit%>
<%end%>
